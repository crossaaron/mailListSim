","original":{"line":38,"column":8}},{"generated":{"line":116,"column":0},"source":"Components/App.js","original":{"line":58,"column":5}},{"name":"Component","generated":{"line":120,"column":2},"source":"Components/App.js","original":{"line":9,"column":33}}],"sources":{"Components/App.js":"import React, { Component, Fragment } from 'react'\nimport { BrowserRouter, Route} from \"react-router-dom\";\n\nimport Inbox from \"./Inbox\";\nimport EmailRead from './EmailRead'\nimport EMAILS from \"../MOCK_DATA\";\nimport Nav from './Nav'\n\nexport default class App extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            emails: EMAILS,\n            isRead: {\n                \"e4cb2243-7453-461c-a824-55ad3cbfd571\": true\n            }\n        };\n\n        this.markRead = this.markRead.bind(this);\n        this.markUnread = this.markUnread.bind(this);\n    }\n\n    markRead(emailId) {\n        let isRead = {...this.state.isRead};\n        isRead[emailId] = true;\n        this.setState({isRead})\n    }\n\n    markUnread(emailId) {\n        let isRead = {...this.state.isRead};\n        isRead[emailId] = false;\n        this.setState({isRead})\n    }\n\n\n    render() {\n\n        return (\n            <div id='app-container'>\n                <BrowserRouter>\n                    <Fragment>\n                        <Nav />\n                        <Route exact path='/' component={() => (\n                            <Inbox emails={this.state.emails}\n                                   isRead={this.state.isRead}\n                                   markRead={this.markRead}\n                                   markUnread={this.markUnread} />\n                        )}/>\n                        <Route exact path='/read/:id' component={() => (\n                            <EmailRead emails={this.state.emails}\n                                       markRead={this.markRead} />\n                            )} />\n                    </Fragment>\n                </BrowserRouter>\n            </div>\n        )\n\n    }\n}"},"lineCount":null}},"hash":"90b8f6784c3ea4ab42f7805a7ecf1dcc","cacheData":{"env":{}}}